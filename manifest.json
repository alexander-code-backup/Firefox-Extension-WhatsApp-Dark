{
  "description": "Ativar tema escuro no Whatsapp Web | Enable dark theme in Whatsapp Web",
  "manifest_version": 2,
  "name": "Whatsapp Dark (Tema Escuro)",
  "author": "Johnendz",
  "version": "3.0",

  "content_scripts": [
    {
      "matches": ["*://*.web.whatsapp.com/*"],
      "css":["style.minified.css"],
	  "run_at": "document_end"
    }
  ],
  
  "icons": {
      "32": "icons/32.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
  }
}